{
  "name": "nest-app",
  "version": "0.0.1",
  "author": "",
  "private": true,
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm -- migration:run",
    "artilery:dino": "artillery dino",
    "artilery:run": "artillery run -o logs/report.json artilery.yml"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/jsonwebtoken": "^8.5.2",
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "fastify-swagger": "^4.8.2",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "nest-winston": "^1.5.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "4.1.6",
    "ts-node": "^10.0.0",
    "tsc": "^2.0.3",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.36",
    "@types/on-finished": "^2.3.1",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "artillery": "^1.7.6",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.7",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1",
    "snyk": "^1.630.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
